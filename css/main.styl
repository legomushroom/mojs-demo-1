use('assets/nesting-tree-traversal.js')

.snowball
  rect 500px
  border-radius 50%
  background white
  position absolute
  bottom -208*PX
  z-index 0
  transform scale(.1)
  transition transform 1s cubic-bezier(0.19, 1, 0.22, 1)
  opacity 0
  &.is-shown
    transform scale(12)
    opacity 1
    z-index 2
  &--by
    right  -208*PX
  &--with
    right  -155*PX

.credits
  position absolute
  right  25px
  bottom 55px
  z-index 1
  color white
  font-family Open Sans, sans-serif
  font-size 8px
  letter-spacing .8px
  padding-left 9.5*gs
  .with
    position absolute
    left 0
.by
  padding-right 3*gs
  &__text
    position relative
    top 1.6*gs
  &__logo
    cursor pointer
    width 18px
    height 24px
    position absolute
    right 0
    top 8*PX
    background-image url("data:image/svg+xml;base64,")
    
.with
  padding-right 4*gs
  &__text
    position relative
    top 1.6*gs
  &__logo
    cursor pointer
    width  31px
    height 35px
    position absolute
    right 0
    background-image url("data:image/svg+xml;base64,")
    
body
  background #333
  font-size 16px

canvasWidth  = 48*gs
canvasHeight = 40*gs
.svg-canvas
  position absolute
  top  50%
  left 50%
  // hl #777
  width  canvasWidth
  height canvasHeight
  margin-top  -(canvasHeight/2)
  margin-left -(canvasWidth/2)
  &__svg
    rect 100%
    position absolute
    nw 0
